                      useRequestIdForTraceSampling:
                        nullable: true
                        type: boolean
                    type: object
                  type: array
              type: object
            status:
              type: object
              x-kubernetes-preserve-unknown-fields: true
          type: object
      served: true
      storage: true
      subresources:
        status: {}
